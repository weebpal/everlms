{#
/**
 * @file
 * Template for the lms_answer add/edit form.
 *
 * Available variables:
 * - form: The form.
 *
 * @ingroup themeable
 */
#}
<div class="layout-lms-answer-form clearfix">
  <div class="layout-region layout-region-lms-answer-main">
    {{ form|without('advanced', 'actions') }}
  </div>
  <div class="layout-region layout-region-lms-answer-secondary">
    {{ form.advanced }}
  </div>
  <div class="layout-region layout-region-lms-answer-footer">
    {{ form.actions }}
  </div>
</div>
